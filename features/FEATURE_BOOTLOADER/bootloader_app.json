{
    "macros": [
        "MBEDTLS_USER_CONFIG_FILE=\"bootloader_mbedtls_user_config.h\"",
        "SHOW_PROGRESS_BAR=0",
        "MAX_COPY_RETRIES=1",
        "MAX_BOOT_RETRIES=3",
        "ARM_BOOTLOADER_USE_NVSTORE_ROT=1",
        "ARM_UC_USE_PAL_CRYPTO=0",
        "ARM_UC_USE_PAL_BLOCKDEVICE=1",
        "ARM_UC_PAAL_TRACE_ENABLE=0",
        "ARM_UC_PROFILE_MBED_CLOUD_CLIENT=1",
        "ARM_UC_FEATURE_CRYPTO_PAL=0",
        "ARM_UC_FEATURE_CRYPTO_MBEDTLS=1",
        "MBED_CLOUD_CLIENT_UPDATE_STORAGE=ARM_UCP_FLASHIAP_BLOCKDEVICE",
        "DEFAULT_MAX_APPLICATION_SIZE=MBED_CONF_BOOTLOADER_MAX_APPLICATION_SIZE",
        "DISABLE_ERROR_DESCRIPTION=1",
        "Mutex=PlatformMutex"
    ],
    "target_overrides": {
        "*": {
            "target.features_add"              : ["BOOTLOADER", "COMMON_PAL"],
            "target.features_remove"           : ["LWIP"],
            "platform.stdio-baud-rate"         : 115200,
            "platform.stdio-flush-at-exit"     : false,
            "nvstore.area_1_address"           : "MBED_CONF_BOOTLOADER_CFG_SECTOR_1_ADDRESS",
            "nvstore.area_1_size"              : "MBED_CONF_BOOTLOADER_CFG_SECTOR_1_SIZE",
            "nvstore.area_2_address"           : "MBED_CONF_BOOTLOADER_CFG_SECTOR_2_ADDRESS",
            "nvstore.area_2_size"              : "MBED_CONF_BOOTLOADER_CFG_SECTOR_2_SIZE",
            "update-client.storage-address"    : "MBED_CONF_BOOTLOADER_STORAGE_ADDRESS",
            "update-client.storage-size"       : "MBED_CONF_BOOTLOADER_STORAGE_SIZE",
            "update-client.storage-locations"  : "MBED_CONF_BOOTLOADER_STORAGE_LOCATIONS",
            "update-client.application-details": "MBED_CONF_BOOTLOADER_APPLICATION_HEADER_ADDRESS",
            "application-start-address"        : "MBED_CONF_BOOTLOADER_APPLICATION_START_ADDRESS",
            "max-application-size"             : "MBED_CONF_BOOTLOADER_MAX_APPLICATION_SIZE",
            "update-client.firmware-header-version": "2",
            "target.bootloader_img"            : null,
            "target.header_offset"             : null,
            "target.app_offset"                : null
        },
        "NUCLEO_F429ZI": {
            "target.components_add"             : ["SD"],
            "sd.SPI_MOSI"                       : "PC_12",
            "sd.SPI_MISO"                       : "PC_11",
            "sd.SPI_CLK"                        : "PC_10",
            "sd.SPI_CS"                         : "PC_9"
        },
        "NUCLEO_F767ZI": {
            "target.components_add"             : ["SD"],
            "sd.SPI_MOSI"                       : "PC_12",
            "sd.SPI_MISO"                       : "PC_11",
            "sd.SPI_CLK"                        : "PC_10",
            "sd.SPI_CS"                         : "PC_9"
        },
        "NUCLEO_F746ZG": {
            "target.components_add"             : ["SD"],
            "sd.SPI_MOSI"                       : "PC_12",
            "sd.SPI_MISO"                       : "PC_11",
            "sd.SPI_CLK"                        : "PC_10",
            "sd.SPI_CS"                         : "PC_9"
        },
        "NUCLEO_F207ZG": {
            "target.components_add"             : ["SD"],
            "sd.SPI_MOSI"                       : "PC_12",
            "sd.SPI_MISO"                       : "PC_11",
            "sd.SPI_CLK"                        : "PC_10",
            "sd.SPI_CS"                         : "PC_9"
        },
        "SDT64B": {
            "target.components_add"             : ["SD"],
            "sd.SPI_MOSI"                       : "PTB16",
            "sd.SPI_MISO"                       : "PTB17",
            "sd.SPI_CLK"                        : "PTB11",
            "sd.SPI_CS"                         : "PTB10"
        },
        "MTB_MXCHIP_EMW3166": {
            "target.components_add"             : ["SPIF"],
            "spif-driver.SPI_MOSI"              : "PB_15",
            "spif-driver.SPI_MISO"              : "PB_14",
            "spif-driver.SPI_CLK"               : "PB_13",
            "spif-driver.SPI_CS"                : "PA_10"
        },
        "MTB_USI_WM_BN_BM_22": {
            "target.components_add"             : ["SPIF"],
            "spif-driver.SPI_MOSI"              : "PC_3",
            "spif-driver.SPI_MISO"              : "PC_2",
            "spif-driver.SPI_CLK"               : "PB_13",
            "spif-driver.SPI_CS"                : "PA_6"
        }
    },
    "config": {
        "application-start-address": {
            "help": "Address to the beginning of the active application firmware in flash",
            "value": null
        },
        "application-jump-address": {
            "help": "Jump address for running the active application firmware",
            "value": null
        },
        "max-application-size": {
            "help": "Maximum size of the active application",
            "value": null
        }
    }
